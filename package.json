{
  "name": "emaily",
  "version": "1.0.0",
  "description": "Email survey web app",
  "main": "index.js",
  "engines": {
    "node": "14.17.6",
    "npm": "8.2.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",

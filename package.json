  "dependencies": {
    "body-parser": "^1.20.0",
    "concurrently": "^7.1.0",
    "cookie-session": "^2.0.0",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "mongoose": "^6.3.0",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "path-parser": "^6.1.0",
    "sendgrid": "^5.2.3",
    "stripe": "^8.218.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
